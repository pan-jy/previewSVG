{
  "name": "previewsvg",
  "displayName": "PreviewSVG",
  "description": "preview svg",
  "version": "0.0.3",
  "publisher": "panjy",
  "author": "panjy",
  "repository": "https://github.com/pan-jy/previewSVG",
  "icon": "public/icon.jpg",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:typescriptreact",
    "onLanguage:xml"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "previewsvg.previewSVGFile",
        "title": "Preview SVG File"
      },
      {
        "command": "previewsvg.previewActiveSVG",
        "title": "Preview Active SVG"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "previewsvg.previewSVGFile",
          "when": "resourceExtname == .svg || resourceExtname == .tsx"
        },
        {
          "command": "previewsvg.previewActiveSVG",
          "when": "resourceExtname == .svg || resourceExtname == .tsx"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.93.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "eslint": "^9.9.1",
    "typescript": "^5.5.4",
    "ts-loader": "^9.5.1",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}